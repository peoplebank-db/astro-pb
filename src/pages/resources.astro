  let description = 'Opinions and advice from the Peoplebank account management team.';
  let permalink = 'https://peoplebank.com/';
---

<html lang="en-GB" dir="ltr">
<head>
  <SEO title={title} description={description} permalink={permalink} />
</head>

<body>
  <Nav />
  <header class="header">
    <div class="width-l">
      <div class="header-content">
        <h1>Resources</h1>
        <p class="tag">Find, engage and hire the best talent for your business with Peoplebank</p>
        <a href="#">Book a Demo</a>
      </div>
    </div>
  </header>
  <main class="main">

  </main>
  <Footer />
</body>
</html>